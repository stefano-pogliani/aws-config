{
  "Version": "2012-10-17",
  "Statement": [{
    "Sid": "AllowBilling",
    "Effect": "Allow",
    "Action": ["aws-portal:*"],
    "Resource": ["*"]
  }, {
    "Sid": "AllowCertificateManager",
    "Effect": "Allow",
    "Action": ["acm:*"],
    "Resource": ["*"]
  }, {
    "Sid": "AllowCloudformation",
    "Effect": "Allow",
    "Action": ["cloudformation:*"],
    "Resource": ["*"]
  }, {
    "Sid": "AllowCloudFront",
    "Effect": "Allow",
    "Action": ["cloudfront:*"],
    "Resource": ["*"]
  }, {
    "Sid": "AllowCloudWatch",
    "Effect": "Allow",
    "Action": ["cloudwatch:*"],
    "Resource": ["*"]
  }, {
    "Sid": "AllowHealth",
    "Effect": "Allow",
    "Action": ["health:*"],
    "Resource": ["*"]
  }, {
    "Sid": "AllowIAM",
    "Effect": "Allow",
    "Action": ["iam:*"],
    "Resource": ["*"]
  }, {
    "Sid": "AllowRoute53",
    "Effect": "Allow",
    "Action": ["route53:*"],
    "Resource": ["*"]
  }, {
    "Sid": "AllowRoute53Domains",
    "Effect": "Allow",
    "Action": ["route53domains:*"],
    "Resource": ["*"]
  }, {
    "Sid": "AllowS3",
    "Effect": "Allow",
    "Action": ["s3:*"],
    "Resource": ["*"]
  }, {
    "Sid": "AllowSes",
    "Effect": "Allow",
    "Action": ["ses:*"],
    "Resource": ["*"]
  }]
}
